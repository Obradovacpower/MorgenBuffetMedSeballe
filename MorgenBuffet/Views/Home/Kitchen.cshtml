@model List<MorgenBuffet.DTO.OrderDTO>
@{
    ViewData["Title"] = "Kitchen";
}

<div>
    <header> Kitchen Orders</header>
    <button>Opdate page</button>
    <form asp-action="GetOrdersOnDate" method="get">
        <div class="form-group">
            <label> Date:</label>
            <input type="date" class="form-control" id="date" name="date" />
        </div>

        <div class="form-group">
            <input type="submit" class="btn btn-success" value="submit" />
        </div>
    </form>
</div>
<table>
    <tr>
        <th>Room Number</th>
        <th>Adults</th>
        <th>Kids</th>
    </tr>
    @foreach (var element in Model)
    {
        <tr>
            <td>@element.RoomNumber</td>
            <td>@element.Adults</td>
            <td>@element.Kids</td>
        </tr>
    }
</table>
